{
  "presets": [
    [
      "env",
      {
        "targets": {
          "browsers": [
            "> 0.5% in my stats",
            "ie >= 9"
          ]
        }
      }
    ],
    "react",
    "stage-0"
  ],
  "plugins": [
    "transform-decorators-legacy",
    "transform-react-remove-prop-types",
    "transform-runtime",
    [
      "module-resolver",
      {
        "alias": {
          "js": "./src/assets/js/",
          "styles": "./src/assets/styles/",
          "config": "./config/",
          "__mocks__": "./__mocks__/"
        }
      }
    ]
  ]
}