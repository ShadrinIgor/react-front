    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "bundle-loader": "^0.5.5",
    "classnames": "^2.2.5",
    "clean-webpack-plugin": "^0.1.17",
    "compass-sass-mixins": "^0.12.7",
    "connect-modrewrite": "^0.10.2",
    "cross-env": "^5.1.3",
