{
  "name": "react-frontend",
  "version": "5.0.0",
  "description": "React-frontend",
  "main": "index.js",
  "node_version": "^5.4.1",
  "scripts": {
    "start": "webpack-dev-server --history-api-fallback -d --progress --colors --hot --inline",
    "start-dist": "cross-env NODE_ENV=production webpack-dev-server --history-api-fallback -p --progress --colors",
    "build": "npm run check && cross-env NODE_ENV=production webpack --progress -p --display-chunks --display-reasons --display-error-details --display-modules",
    "test": "jest --verbose --bail",
    "lint": "./node_modules/.bin/eslint src/assets/js/ --ext .jsx,.js",
    "check": "npm run lint && npm run test"
  },
  "pre-commit": [
    "check"
  ],
  "jest": {
    "moduleDirectories": [
      "node_modules",
      "src/assets"
    ],
    "moduleNameMapper": {
      "\\.(css|less|sass|scss)$": "<rootDir>/__mocks__/styleMock.js",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "setupFiles": [
      "./config/test-env.js"
    ]
  },
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^7.2.3",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.1.2",
